  <joint name="end_effector_cam_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0.021 0"/>
    <parent link="gripper_camera_mount_link"/>
    <child link="end_effector_cam_link"/>
  </joint>
  <link name="end_effector_cam_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1e-5"/>
      <inertia ixx="1e-6" ixy="0.00000000000" ixz="0.00000002565" iyy="1e-6" iyz="0.00000000000" izz="1e-6"/>
    </inertial>
