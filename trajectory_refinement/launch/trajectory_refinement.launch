<launch>

<arg name="refinement_stiffness" default="0.05"/>
<arg name="refinement_damping_coefficient" default="0.001"/>

<include file="$(find dmp)/launch/dmp.launch"/>
<include file="$(find refinement_force_publisher)/launch/refinement_force_publisher.launch" >
    <arg name="refinement_stiffness" value="$(arg refinement_stiffness)"/>
    <arg name="refinement_damping_coefficient" value="$(arg refinement_damping_coefficient)"/>

</include>

<include file="$(find trajectory_visualizer)/launch/trajectory_visualizer.launch"/> 
<include file="$(find trajectory_refinement)/launch/gripper_wrt_ee_publisher.launch"/> 
<include file="$(find aruco_ros)/launch/marker_publisher.launch"/>

<include file="$(find teleop_control)/launch/teleop_control.launch" >
    <arg name="part_to_publish" value="position" />
</include>

<node pkg="trajectory_refinement" name="trajectory_refinement" 
type="trajectory_refinement_promp.py" output="screen">
</node>

</launch>